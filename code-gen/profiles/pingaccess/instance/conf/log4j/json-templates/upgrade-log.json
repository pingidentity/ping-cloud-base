{
  "timestamp": {
    "$resolver": "timestamp",
    "pattern": { "format": "yyyy-MM-dd'T'HH:mm:ss.SSSXX" }
  },
  "logLevel": {
    "$resolver": "level"
  },
  "className": {
    "$resolver": "logger",
    "field": "name"
  },
  "position": {
    "$resolver": "source",
    "field": "line"
  },
  "message": {
    "$resolver": "message"
  },
  "log_name": "pingaccess_upgrade"
}
