            "keyword": {
              "ignore_above": 256,
              "type": "keyword"
            }
          }
        },
        "severity": {
          "type": "long"
        },
        "geoip": {
          "dynamic": true,
          "properties": {
            "ip": {
              "type": "ip"
            },
            "latitude": {
