{
  "id": "${DA_JWT_ID}",
  "name": "${DA_JWT_ID}",
  "pluginDescriptorRef": {
    "id": "com.pingidentity.pf.access.token.management.plugins.JwtBearerAccessTokenManagementPlugin"
  },
  "configuration": {
    "tables": [{
      "name": "Symmetric Keys",
      "rows": [{
        "fields": [{
            "name": "Key ID",
            "value": "dakey"
          },
          {
            "name": "Key",
            "value": "${DA_JWT_SYMMETRIC_KEY}"
          }
        ]
      }]
    }],
    "fields": [{
        "name": "JWS Algorithm",
        "value": "HS256"
      },
      {
        "name": "Active Symmetric Key ID",
        "value": "dakey"
      }
    ]
  },
  "attributeContract": {
    "extendedAttributes": [{
      "name": "sub"
    }]
  },
  "sessionValidationSettings": {
    "includeSessionId": false,
    "checkValidAuthnSession": true,
    "checkSessionRevocationStatus": true,
    "updateAuthnSessionActivity": true
  }
}